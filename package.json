{
  "name": "trending-news",
  "version": "0.0.1",
  "description": "Get news, filter by score, push to phone",
  "main": "index.js",
  "precommit": [
    "lint",
    "compile",
    "test"
  ],
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint src/*",
    "compile": "./node_modules/.bin/grunt coffee",
    "test": "./node_modules/.bin/grunt jasmine_node | node_modules/.bin/bunyan",
    "prepublish": "npm run compile",
    "postinstall": "./node_modules/.bin/grunt hookmeup",
    "start": "node index.js | node_modules/.bin/bunyan",
    "startprod": "./node_modules/.bin/forever start index.js 'prod'",
    "startdebug": "./node_modules/.bin/forever start index.js 'debug'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iconix/tn.git"
  },
  "keywords": [
    "news",
    "trending",
    "techm"
  ],
  "author": "iconix",
  "bugs": {
    "url": "https://github.com/iconix/tn/issues"
  },
  "homepage": "https://github.com/iconix/tn",
  "dependencies": {
    "bunyan": "^1.1.0",
    "forever": "^0.11.1",
    "instapush": "0.0.7",
    "loggly": "^1.0.5",
    "nconf": "^0.6.9",
    "node-persist": "0.0.2",
    "node-uuid": "^1.4.1"
  },
  "devDependencies": {
    "coffee-script": "^1.8.0",
    "coffeelint": "^1.6.0",
    "grunt": "^0.4.5",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-coffee": "^0.11.1",
    "grunt-jasmine-node": "^0.2.1",
    "grunt-jsdoc": "^0.5.7",
    "grunt-shell": "^1.1.1",
    "jasmine-node": "^1.14.5",
    "jshint": "^2.5.5",
    "nock": "^0.46.0",
    "precommit-hook": "latest"
  }
}

{
  "name": "gith-deployer",
  "version": "0.0.0",
  "description": "GitHub WebHooks + Gith = Automatic Deployments",
  "main": "hook.js",
  "scripts": {
    "start": "./node_modules/.bin/forever -a start hook.js",
    "re-start": "./node_modules/.bin/forever -a restart hook.js",
    "stop": "./node_modules/.bin/forever stop hook.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/iconix/tn.git"
  },
  "keywords": [
    "deploy",
    "gith",
    "webhooks",
    "continuous"
  ],
  "author": "iconix",
  "bugs": {
    "url": "https://github.com/iconix/tn/issues"
  },
  "homepage": "https://github.com/iconix/tn",
  "dependencies": {
    "bunyan": "^1.1.3",
    "forever": "^0.11.1",
    "gith": "^1.0.4"
  },
  "devDependencies": {}
}
